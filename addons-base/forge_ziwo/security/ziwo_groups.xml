<odoo>
    <data>
        <record id="group_ziwo_agent" model="res.groups">
            <field name="name">Ziwo Agent</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
        <record id="group_ziwo_manager" model="res.groups">
            <field name="name">Ziwo Manager</field>
            <field name="category_id" ref="base.module_category_hidden"/>
            <field name="implied_ids" eval="[(4, ref('group_ziwo_agent'))]"/>
        </record>
        <record id="base.group_system" model="res.groups">
            <field name="implied_ids" eval="[(4, ref('group_ziwo_manager'))]"/>
        </record>
    </data>
</odoo>